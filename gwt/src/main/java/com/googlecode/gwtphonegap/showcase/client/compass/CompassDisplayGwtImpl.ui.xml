<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">

    <mgwt:LayoutPanel>
        <mgwt:HeaderPanel>
            <mgwt:left>
                <mgwt:HeaderButton ui:field="backButton" backButton="true" text="Modules"></mgwt:HeaderButton>
            </mgwt:left>
            <mgwt:center>
                <g:HTML>Compass</g:HTML>
            </mgwt:center>
        </mgwt:HeaderPanel>

        <mgwt:ScrollPanel scrollingEnabledX="false">
            <g:FlowPanel>
                <mgwt:WidgetList round="true">
                    <g:HorizontalPanel>
                        <g:HTML>Magnetic Heading:</g:HTML>
                        <g:HTML ui:field="mh"></g:HTML>
                    </g:HorizontalPanel>
                    <g:HorizontalPanel>
                        <g:HTML>True Heading:</g:HTML>
                        <g:HTML ui:field="th"></g:HTML>
                    </g:HorizontalPanel>
                    <g:HorizontalPanel>
                        <g:HTML>heading Accuracy</g:HTML>
                        <g:HTML ui:field="ha"></g:HTML>
                    </g:HorizontalPanel>
                    <g:HorizontalPanel>
                        <g:HTML>Time:</g:HTML>
                        <g:HTML ui:field="timestamp"></g:HTML>
                    </g:HorizontalPanel>
                </mgwt:WidgetList>

                <mgwt:Button ui:field="startButton"></mgwt:Button>
            </g:FlowPanel>
        </mgwt:ScrollPanel>

    </mgwt:LayoutPanel>

</ui:UiBinder> 