<module rename-to="showcase">


    <inherits name="com.googlecode.mgwt.MGWT"/>

    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.resources.Resources" />
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="com.google.maps.gwt.GoogleMaps" />
    <inherits name="com.googlecode.mgwt.MGWT"/>
    <inherits name='com.googlecode.gwtphonegap.PhoneGap'/>
    <inherits name="com.google.gwt.i18n.I18N"/>

    <inherits name="com.bikefunfinder.BikeFunFinder" />


    <add-linker name="xsiframe"/>

    <set-property name="user.agent" value="safari,gecko1_8"/>
    <set-property name="mgwt.os" value="android, desktop" />

    <!--<set-configuration-property name="mgwt.css" value="pretty" />-->

    <!-- 	<inherits name="com.google.gwt.core.Core" /> -->
