<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget"
             xmlns:mgwtb="urn:import:com.googlecode.mgwt.ui.client.widget.buttonbar">

    <mgwt:LayoutPanel>
        <mgwt:HeaderPanel>
            <mgwt:left>
                <mgwt:HeaderButton ui:field="backButton" backButton="true" text="Modules"></mgwt:HeaderButton>
            </mgwt:left>
            <mgwt:center>
                <g:HTML>Notification</g:HTML>
            </mgwt:center>
        </mgwt:HeaderPanel>

        <mgwt:ScrollPanel scrollingEnabledX="false">
            <g:FlowPanel>


                <mgwt:Button ui:field="alert" text="Alert"></mgwt:Button>
                <mgwt:Button ui:field="confirm" text="Confirm"></mgwt:Button>
                <mgwt:Button ui:field="vibrate" text="Vibrate"></mgwt:Button>
                <mgwt:Button ui:field="beep" text="Beep"></mgwt:Button>


            </g:FlowPanel>
        </mgwt:ScrollPanel>


    </mgwt:LayoutPanel>


</ui:UiBinder> 