<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget" xmlns:mgwtb="urn:import:com.googlecode.mgwt.ui.client.widget.buttonbar">
	
	<mgwt:LayoutPanel>
		<mgwt:HeaderPanel>
			<mgwt:left><mgwt:HeaderButton ui:field="backButton" backButton="true" text="Modules"></mgwt:HeaderButton></mgwt:left>
			<mgwt:center><g:HTML>Media</g:HTML></mgwt:center>
		</mgwt:HeaderPanel>
		
		<mgwt:ScrollPanel scrollingEnabledX="false">
			<g:FlowPanel>
				
				
						
						<g:HTML ui:field="position"></g:HTML>
					
					
				
				
				
			</g:FlowPanel>
		</mgwt:ScrollPanel>
		
		<mgwtb:ButtonBar>
			<mgwtb:ButtonBarSpacer></mgwtb:ButtonBarSpacer>
			<mgwtb:PlayButton ui:field="play"></mgwtb:PlayButton>
			<mgwtb:PauseButton ui:field="pause"></mgwtb:PauseButton>
			<mgwtb:ButtonBarSpacer></mgwtb:ButtonBarSpacer>
		</mgwtb:ButtonBar>
		
	</mgwt:LayoutPanel>
	
	
</ui:UiBinder> 